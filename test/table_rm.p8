__lua__
function _init()
	local tab={2,4,4,5,36}

	cls()

	print("--regular--")
	printall(tab)
	print("--remove---")
	rm(tab)
	printall(tab)
	print("--add------")
	ad(tab)
	printall(tab)
end

function ad(tab)
	add(tab,2)
	add(tab,3)
end

function rm(tab)
	local co=0
	for i in all(tab) do co+=1 end
	for n=1,co,1 do tab[n]=nil end
end

function printall(tab)
	for i in all(tab) do print(i) end
	if(tab[1]==nil) then print("nothing") end
end
