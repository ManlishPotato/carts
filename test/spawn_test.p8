end

function addObj()
 if(btnp(4)) then
  add(ob.tbl,flr(rnd(128))) --x
  add(ob.tbl,flr(rnd(128))) --y
  add(ob.tbl,2)
 end
end

function drawObj()
 local count=0
 for i in all(ob.tbl) do count+=1 end
 for n=3,count,3 do
  spr(ob.tbl[n],ob.tbl[n-2],ob.tbl[n-1])
 end
end
