pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
xpos=64
ypos=64
rad=0
angle=0

cls()

function _update60()
	for i=0,50,1 do
		pset(flr((cos(angle/360)*rad+xpos)+0.5),flr((sin(angle/360)*rad+ypos)+0.5),8)
		
		if(angle>359) then angle=0
		else	angle+=1 end
		
		rad+=0.01
	end
end
