pico-8 cartridge // http://www.pico-8.com
version 18
__lua__
p1={}
p1.x=0
p1.y=0
p1.lastx=0
p1.lasty=10

p1.spr=18
p1.r=0
p1.lastr=0
p1.sel=0
p1.grav=0.25

debug_b=0
debug_a=false

-- start pos | row1 | row2
--tab={px,py, cx,cy,n, cx,cy,n}
tet_i={56,0, -8,0,4, "nr"} --1
tet_t={56,0, -8,0,3, 0,8,1} --2
tet_o={56,0, -8,0,2, -8,8,2} --3
tet_s={56,0, -8,0,2, -16,8,2} --4
tet_z={56,0, -8,0,2, 0,8,2} --5
tet_l={56,0, -8,0,3, -8,8,1} --6
tet_j={56,0, -8,0,3, 8,8,1} --7

function _init()
	table=tet_t
	debug_table=tet_j
	p1.x,p1.y=table[1],table[2]
end

function _update60()
	cls()
	map(0,0,0,0,16,16)



	move()
	collision("move")
	rotate()
	collision("rot")

	gravity()
	collision("gravity")

	tetri("draw")
	map(0,0,0,0,16,16)
end


function mapset()
	local pos_x=0
	local pos_y=0
	local rot_x=0
 local rot_y=0

 for i=1,table[5],1 do
 	rot_x,rot_y=rot(table[3]+pos_x,table[4]+pos_y)
		mset((p1.x+rot_x)/8,flr(((p1.y+rot_y)/8)+0.5),p1.spr)
 	pos_x+=8
 end

 if(table[6]!="nr") then
 	pos_x=0
 	pos_y=0

		for n=1,table[8],1 do
			rot_x,rot_y=rot(table[6]+pos_x,table[7]+pos_y)
			mset((p1.x+rot_x)/8,flr(((p1.y+rot_y)/8)+0.5),p1.spr)
			pos_x+=8
		end
	end
end

function init_tet()
	sfx(0)
	mapset()

	p1.spr=flr(rnd(6))+18
	p1.sel=flr(rnd(7))+1
	p1.r=0

	if(p1.sel==1) then table=tet_i end
	if(p1.sel==2) then table=tet_t end
	if(p1.sel==3) then table=tet_o end
	if(p1.sel==4) then table=tet_s end
	if(p1.sel==5) then table=tet_z end
	if(p1.sel==6) then table=tet_l end
	if(p1.sel==7) then table=tet_j end

	p1.x,p1.y=table[1],table[2]
end

function gravity()
	p1.lastx,p1.lasty=p1.x,p1.y
	if(btn(3)) then p1.y+=p1.grav+4 else p1.y+=p1.grav end
end

function move()
	if(btnp(0)) then p1.x-=8 end
	if(btnp(1)) then p1.x+=8 end
end

function rotate()
	p1.lastr=p1.r

	if(btnp(4)) then p1.r-=90 end
	if(btnp(5)) then p1.r+=90 end
	if(p1.r>270) then p1.r=0 end
	if(p1.r<0) then p1.r=270 end
end

function collision(op)
	local pos_x=0
	local pos_y=0
	local rot_x=0
 local rot_y=0

 for i=1,table[5],1 do
 	rot_x,rot_y=rot(table[3]+pos_x,table[4]+pos_y)
 	for k=0,7,7 do
 		for l=0,7,7 do
				if(op=="gravity") then
					flag=fget(mget((p1.x+rot_x+l)/8,(p1.y+rot_y+k)/8),0)
	 			if(flag) then
						p1.y=p1.lasty
						init_tet()
						goto continue_init
	 			end
				end
				if(op=="move") then
	 			flag=fget(mget((p1.x+rot_x+l)/8,(p1.y+rot_y+k)/8),0)
	 			if(flag) then
	 				p1.x=p1.lastx
	 			end
			 end
				if(op=="rot") then
					flag=fget(mget((p1.x+rot_x+l)/8,(p1.y+rot_y+k)/8),0)
					if(flag) then
						p1.r=p1.lastr
				 end
				end
   end
	 end
		pos_x+=8
 end

 if(table[6] !="nr") then
 	pos_x,pos_y=0,0
		for n=1,table[8],1 do
			rot_x,rot_y=rot(table[6]+pos_x,table[7]+pos_y)
			for v=0,7,7 do
	 		for b=0,7,7 do
					if(op=="gravity") then
						flag=fget(mget((p1.x+rot_x+b)/8,(p1.y+rot_y+v)/8),0)
		 			if(flag) then
							p1.y=p1.lasty
		 				init_tet()
							goto continue_init
		 			end
					end
					if(op=="move") then
		 			flag=fget(mget((p1.x+rot_x+b)/8,(p1.y+rot_y+v)/8),0)
		 			if(flag) then
		 				p1.x=p1.lastx
		 			end
					end
					if(op=="rot") then
						flag=fget(mget((p1.x+rot_x+b)/8,(p1.y+rot_y+v)/8),0)
						if(flag) then
							p1.r=p1.lastr
						end
					end
 			end
 		end
		pos_x+=8
	 end
 end
	::continue_init::
end

function tetri(op)
	local pos_x=0
	local pos_y=0
	local rot_x=0
 local rot_y=0

 for i=1,table[5],1 do
 	rot_x,rot_y=rot(table[3]+pos_x,table[4]+pos_y)
		if(op=="draw") then spr(p1.spr,p1.x+rot_x,p1.y+rot_y) end
 	pos_x+=8
 end
 if(table[6] != "nr") then
 	pos_x, pos_y=0,0
		for n=1,table[8],1 do
			rot_x,rot_y=rot(table[6]+pos_x,table[7]+pos_y)
			if(op=="draw") then spr(p1.spr,p1.x+rot_x,p1.y+rot_y) end
			pos_x+=8
		end
 end
end

function rot(val_x,val_y)
 if(p1.r==0) then
 	return val_x,val_y
	end

	if(p1.r==90) then
	 return (val_y)*-1,val_x
	end

	if(p1.r==180) then
		return (val_x)*-1,(val_y)*-1
	end

	if(p1.r==270) then
	 return val_y,(val_x)*-1
	end
end

