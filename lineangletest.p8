pico-8 cartridge // http://www.pico-8.com
version 36
__lua__
agl=0

function _update60()
	cls()

	rad=30
	cx=63
	cy=63

	circ(cx,cy,rad,7)

	for i=0,1,1 do
		x2=cos(agl/360)*rad+cx
		y2=sin(agl/360)*rad+cy
		x1=cos((180+agl)/360)*rad+cx
		y1=sin((180+agl)/360)*rad+cy

		line(x1,y1,x2,y2,8)
	end

	if(agl>359) then agl=0
	else agl+=1 end
end
